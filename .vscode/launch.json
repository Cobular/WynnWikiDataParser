  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug GenerateNodeData",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/generateNodeData.ts",
      "cwd": "${workspaceRoot}",
      "preLaunchTask": "tsc",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug CleanNodeData",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/cleanNodeData.ts",
      "cwd": "${workspaceRoot}",
      "preLaunchTask": "tsc",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Test",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/test.ts",
      "cwd": "${workspaceRoot}",
      "preLaunchTask": "tsc",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "sourceMaps": true
    }
  ]
}
